# Default server configuration
#
server {
    listen 443 ssl;
    listen 80;
    ssl_certificate /etc/nginx/certificate.pem;
    ssl_certificate_key /etc/nginx/certificate.key;
    root /var/www/html;
    index index.html;
    server_name _;
    location / {
            try_files $uri $uri/ =404;
    }
}
